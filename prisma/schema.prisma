generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Image {
  id           String   @id @default(cuid())
  title        String
  alt          String
  src          String
  photographer String
  email        String?
  website      String?
  instagram    String?
  twitter      String?
  category     String
  size         String
  price        Float?
  userId       String? @default(cuid())
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}

