generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Image {
  id           String   @id @default(cuid())
  title        String
  alt          String
  src          String
  photographer String
  category     String
  size         String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}

